{% schema %}
{
  "name": "Top Best Seller",
  "tag": "section",
  "class": "bestseller-section",
  "settings": [
    {
      "type": "text",
      "id": "section_title",
      "label": "Section Title",
      "default": "TOP BEST SELLER"
    }
  ],
  "blocks": [
    {
      "type": "product",
      "name": "Product",
      "settings": [
        {
          "type": "image_picker",
          "id": "product_image",
          "label": "Product Image"
        },
        {
          "type": "text",
          "id": "product_title",
          "label": "Product Title",
          "default": "Product Name"
        },
        {
          "type": "url",
          "id": "product_url",
          "label": "Product URL"
        },
        {
          "type": "text",
          "id": "sale_price",
          "label": "Sale Price",
          "default": "13.97",
          "info": "Enter price without currency symbol"
        },
        {
          "type": "text",
          "id": "original_price",
          "label": "Original Price",
          "default": "26.79",
          "info": "Enter price without currency symbol"
        },
        {
          "type": "number",
          "id": "discount_percentage",
          "label": "Discount Percentage",
          "default": 48,
          "info": "Enter number without % symbol"
        },
        {
          "type": "text",
          "id": "button_text",
          "label": "Button Text",
          "default": "Add to cart"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "Top Best Seller",
      "blocks": [
        {
          "type": "product",
          "settings": {
            "product_title": "Hydro Boost Night Pressed Serum",
            "sale_price": "13.97",
            "original_price": "26.79",
            "discount_percentage": 48,
            "button_text": "Add to cart"
          }
        },
        {
          "type": "product",
          "settings": {
            "product_title": "Skin Renewing Nightly Treatment",
            "sale_price": "23.86",
            "original_price": "29.99",
            "discount_percentage": 20,
            "button_text": "Add to cart"
          }
        },
        {
          "type": "product",
          "settings": {
            "product_title": "Daily Collagen Face Moisturizer",
            "sale_price": "22.49",
            "original_price": "51.91",
            "discount_percentage": 57,
            "button_text": "Add to cart"
          }
        }
      ]
    }
  ]
}
{% endschema %}

